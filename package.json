{
  "name": "cloud-security-scanner-cli",
  "version": "0.0.2",
  "description": "A security tool to scan cloud storage (Google Drive and Dropbox) for potential vulnerabilities and exposed credentials",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --watch src/index.js",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "release:pre": "standard-version --prerelease"
  },
  "keywords": [
    "google-drive",
    "dropbox",
    "cloud-storage",
    "security",
    "vulnerability",
    "scanner",
    "credentials",
    "multi-cloud"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "dropbox": "^10.34.0",
    "googleapis": "^142.0.0",
    "ora": "^8.0.1"
  },
  "devDependencies": {
    "standard-version": "^9.5.0"
  }
}
